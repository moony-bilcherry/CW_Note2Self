<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="LightPurple" >#8980f5</Color>
    <SolidColorBrush x:Key="LightPurpleBrush" Color="{StaticResource LightPurple}"/>

    <Color x:Key="OffWhite" >#fdfffc</Color>
    <SolidColorBrush x:Key="OffWhiteBrush" Color="{StaticResource OffWhite}"/>

    <Color x:Key="Teal" >#2ec4b6</Color>
    <SolidColorBrush x:Key="TealBrush" Color="{StaticResource Teal}"/>

    <Color x:Key="OffBlack" >#011627</Color>
    <SolidColorBrush x:Key="OffBlackBrush" Color="{StaticResource OffBlack}"/>

    <Color x:Key="Crimson" >#e71d36</Color>
    <SolidColorBrush x:Key="CrimsonBrush" Color="{StaticResource Crimson}"/>

    <Color x:Key="Yellow" >#ffc857</Color>
    <SolidColorBrush x:Key="YellowBrush" Color="{StaticResource Yellow}"/>

</ResourceDictionary>